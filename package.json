{
  "name": "@nextnm/nestjs-mailgun",
  "version": "3.0.1",
  "description": "NestJS provider for sending emails with mailgun",
  "author": "Nuno Carvalh√£o <nuno.carvalhao@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "mailgun",
    "emails"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextNC/nestjs-mailgun"
  },
  "bugs": {
    "url": "https://github.com/NextNC/nestjs-mailgun"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.1"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "mailgun.js": "^9.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/testing": "10.2.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.4",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.8",
    "jest": "29.6.4",
    "prettier": "2.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/nestjsplus/nestjs-package-starter#readme"
}
